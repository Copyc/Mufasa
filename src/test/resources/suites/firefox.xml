<?xml version="1.0"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >
<suite name="Chrome">

	<parameter name="browser" value="firefox"/>
	<suite-files>
		<suite-file path="common.xml"></suite-file>
	</suite-files>
	
	<groups>
		<run>
			<exclude name="runOnFirefox"></exclude>
			<include name="doNotRunOnFirefox"></include>
		</run>
	</groups>
	
</suite>

